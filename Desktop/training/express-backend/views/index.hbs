<html>
  <head>
    <title>Doko Online Store</title>  
    <link rel="stylesheet" href="./css/index.css">
  </head>
  <body>
      
{{>header}}

<section class="products">
   {{#each productData}}
  <div class="product-card">
    <a href="/details.html">
    <div class="product-image">
      <img src="{{image}}">
    </div>
    <div class="product-info">
      <h5>{{title}}</h5>
      <h6>{{price}}</h6>
      <p>{{description}}</p>
      <p> {{slug}}</p>
         <p> {{id}}</p>
    </div>
  </a>
  </div>
  {{/each}}
</section>

</body>
</html>